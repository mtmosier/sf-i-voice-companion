        string filepath = GetKnownFolderPath(localLowId);
        filepath = Path.Combine(filepath, "Ben Olding Games");
        filepath = Path.Combine(filepath, "Starfighter_ Infinity");
        filepath = Path.Combine(filepath, "steam_global.dat");
        return filepath;
