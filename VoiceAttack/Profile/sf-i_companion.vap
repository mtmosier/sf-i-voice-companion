                                settingName = fromVarName + ".weaponKeyPress[" + l + "]";
                                string keybind = VA.GetText(settingName);
                                VA.SetText(toVarName + ".weaponKeyPress[" + l + "]", keybind);

                                settingName = fromVarName + ".weaponKeyPressFriendly[" + l + "]";
