            // VA.WriteToLog("Planets found " + VA.GetInt("&gt;&gt;codexPlanetDescription.len").ToString(), "Red");
            // VA.WriteToLog("Objects found " + VA.GetInt("&gt;&gt;codexObjectDescription.len").ToString(), "Red");
        }
        reader.Close();
        dataStream.Close();
        response.Close();
    }
