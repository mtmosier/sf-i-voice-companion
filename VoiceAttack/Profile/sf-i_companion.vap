									}

									if (!string.IsNullOrEmpty(keybind)) {
										VA.SetText(sName, keybind);
									}
								}
							}
						}
					}



					foreach (string gName in staticGroupList) {
						tmpVarName = "&gt;&gt;shipInfo[" + fullShipList[s] + "].weaponGroup[" + gName + "]";

