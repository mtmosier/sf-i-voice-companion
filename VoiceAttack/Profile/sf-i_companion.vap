		tmpCmdId = VA.GetText("&gt;reloadProfileCommandId");
		if (!string.IsNullOrEmpty(tmpCmdId))  reloadProfileGuid = new Guid(tmpCmdId);

		tmpCmdId = VA.GetText("&gt;playRandomSoundCommandId");
