          <Context>using System;
using System.Globalization;
using System.Text.RegularExpressions;

public class VAInline
{
	private Guid playRandomSoundGuid;
	private Guid requestVerbalUserInputGuid;
	private Guid writeSettingsToFileGuid;

	private void cancelConfiguration() {
		playRandomSound("Cancelling Configuration", "Cancel", false);
	}

	private void configurationError() {
		cancelConfiguration();
//		playRandomSound("Configuration error. Cancelling", "General Error", false);
	}

	private void timeoutError() {
		playRandomSound("Configuration error. Timed out.", "General Error", false);
	}

	public void main()
	{
		//*** INITIALIZE
		TextInfo ti = CultureInfo.CurrentCulture.TextInfo;
		string groupName;
		string groupNum;

		string[] agreeList = { "confirm", "positive", "affirmative", "absolutely", "please", "please do", "yeah", "yes", "yessir", "yes sir", "commit" };
		string[] saveList = { "save", "please save" };
		string[] disagreeList = { "no", "nah", "nope", "negative" };
		string[] cancelList = { "cancel", "stop", "nevermind", "abort" };


		//*** GET RELEVANT SETTINGS
		string activeShipName = VA.GetText("&gt;&gt;activeShipName");
		bool? headphonesInUse = VA.GetBoolean("&gt;&gt;headphonesInUse");

		string tmpCmdId = VA.GetText("&gt;playRandomSoundCommandId");
		if (!string.IsNullOrEmpty(tmpCmdId))  playRandomSoundGuid = new Guid(tmpCmdId);

		tmpCmdId = VA.GetText("&gt;requestVerbalUserInputCommandId");
		if (!string.IsNullOrEmpty(tmpCmdId))  requestVerbalUserInputGuid = new Guid(tmpCmdId);

		tmpCmdId = VA.GetText("&gt;writeSettingsToFileCommandId");
		if (!string.IsNullOrEmpty(tmpCmdId))  writeSettingsToFileGuid = new Guid(tmpCmdId);


		//*** GET PARAMETERS
		bool? quickConfig = VA.GetBoolean("~~quickConfig");
		if (quickConfig != true) {
			groupName = VA.Command.Segment(1);
			groupNum = VA.Command.Segment(2);
		} else {
			groupName = VA.GetText("~~wgGroupNameExtStr");
			groupNum = VA.GetText("~~wgGroupNumExtStr");
		}

		if (string.IsNullOrEmpty(groupName) || groupName.Equals("Group", StringComparison.OrdinalIgnoreCase) || groupName.Equals("Weapon Group", StringComparison.OrdinalIgnoreCase))
			groupName = "Default";
		groupName = ti.ToTitleCase(groupName);

		if (string.IsNullOrEmpty(groupNum))
			groupNum = "1";
		if (groupName.Equals("eighths", StringComparison.OrdinalIgnoreCase))
			groupNum = "8";
		if (groupName.Equals("ate", StringComparison.OrdinalIgnoreCase))
			groupNum = "8";


		//*** START PLAYER INTERACTION
		if (quickConfig != true)
			playRandomSound(null, "Configure Weapon Group", true);


		//*** ASK THE USER FOR A WEAPON COUNT
		string playbackText;
		if (quickConfig != true)	playbackText = "How many weapons will be part of group " + (groupName == "Default" ? "" : groupName) + " " + groupNum + "?";
		else						playbackText = (groupName == "Default" ? "Group" : groupName) + " " + groupNum + " weapon count?";
		string response = getUserInput(
			"[1..20;ate;eighths] [weapon;weapons;];" + string.Join(";", cancelList),
			playbackText,
			null,
			false
		);

		if (string.IsNullOrEmpty(response)) {
			timeoutError();
			return;
		}

		if (Array.IndexOf(cancelList, response) != -1) {
			cancelConfiguration();
			return;
		}

		response = response.Replace("eighths", "8");
		response = response.Replace("ate", "8");
		response = Regex.Replace(response, "[^0-9]", "");
		int weaponCount = getIntFromStr(response);
		if (weaponCount == -1) {
			configurationError();
			return;
		}
		VA.WriteToLog("Using weapon count " + weaponCount, "Black");

		string request = "";
		if (quickConfig != true)
			request += weaponCount + " weapon" + (weaponCount != 1 ? "s" : "") + " to be added. ";

		string[] keyPressList = new string[weaponCount];
		string[] weaponList = new string[weaponCount];
		for (short i = 1; i &lt;= weaponCount; i++) {
			if (i == 1) {
				if (quickConfig != true) {
					request += "Which weapon do you want to fire";
					if (weaponCount &gt; 1)  request += " " + VA.GetText("&gt;ordList(" + i + ")") + "?";
					else request += "?";
				} else {
					request += VA.GetText("&gt;ordList(" + i + ")") + " weapon?";
			}
			} else {
				request = VA.GetText("&gt;ordList(" + i + ")") + "?";
			}

			response = getUserInput(
				"[weapon;] [slot;] [0..9;ate;eighths];augmentation;augment;propulsion [enhancer;];primary [weapon;];" + string.Join(";", cancelList),
				request,
				null,
				(headphonesInUse != true)
			);


			if (string.IsNullOrEmpty(response)) {
				timeoutError();
				return;
			}

			if (Array.IndexOf(cancelList, response) != -1) {
				cancelConfiguration();
				return;
			}

			switch (response) {
	            case "augmentation":
	            case "augment":
					keyPressList[i - 1] = "UseAugmentation";
					weaponList[i - 1] = "augmentation";
	                break;
	            case "propulsion":
	            case "propulsion enhancer":
					keyPressList[i - 1] = "UsePropulsionEnhancer";
					weaponList[i - 1] = "propulsion enhancer";
	                break;
				case "primary":
				case "primary weapon":
					keyPressList[i - 1] = "PrimaryFire";
					weaponList[i - 1] = "primary weapon";
	                break;
	            default:
					response = response.Replace("eighths", "8");
					response = response.Replace("ate", "8");
					keyPressList[i - 1] = "Slot" + Regex.Replace(response, "[^0-9]", "");
					weaponList[i - 1] = keyPressList[i - 1].Replace("Slot", "Slot ");
	                break;
	        }
		}

		if (quickConfig != true) {
			request = "Weapon group " + (groupName == "Default" ? "" : groupName) + " " + groupNum + " will fire ";
			request += string.Join(", ", weaponList);
			request += ". Do you want to save this weapon group?";
		} else {
			request = "Save weapon group?";
 		}

		response = getUserInput(
			string.Join(";", agreeList)
				+ ";" + string.Join(";", saveList)
				+ ";" + string.Join(";", disagreeList)
				+ ";" + string.Join(";", cancelList),
			request,
			null,
			(headphonesInUse != true)
		);

		if (string.IsNullOrEmpty(response)) {
			timeoutError();
			return;
		}
		if (Array.IndexOf(cancelList, response) != -1 || Array.IndexOf(disagreeList, response) != -1) {
			cancelConfiguration();
			return;
		}

		VA.WriteToLog("Saving group " + (groupName == "Default" ? "" : groupName) + " " + groupNum, "Blue");

		string wgDefVarName = "&gt;&gt;shipInfo["+activeShipName+"].weaponGroup["+groupName+"]["+groupNum+"]";

		VA.SetBoolean(wgDefVarName+".isActive", true);
		VA.SetInt(wgDefVarName+".weaponKeyPress.len", weaponCount);
		for (short i = 0; i &lt; weaponCount; i++) {
			VA.SetText(wgDefVarName+".weaponKeyPress["+i+"]", keyPressList[i]);
		}

		if (writeSettings()) {
			playRandomSound("Configuration [saved;complete]", "Configuration Complete");
		} else {
			configurationError();
		}
	}




	private int getIntFromStr(string input)
	{
		if (input == null || input == "") {
			return -1;
		} else {
			try { return Convert.ToInt32(input); }
			catch (FormatException) { return -1; }
			catch (OverflowException) { return -1; }
		}
	}

	private string getUserInput(string inputOptionListStr, string playbackText, string playbackFileGroupName, bool pauseForPlayback = false) {
