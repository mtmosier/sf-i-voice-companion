				+ string.Join(";", restartList),  //*** inputOptionListStr
				request,  //*** playbackText
				null,  //*** playbackFileGroupName
				(headphonesInUse != true), //*** pauseForPlayback
				false, //*** shortPause
				allowFreeFormShipNames   //*** returnOnAnyInput
