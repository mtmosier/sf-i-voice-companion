		playRandomSound("Active ship changed to " + toShip, null, true);

		if (writeSettings()) {
			playRandomSound("Copy complete", "Configuration Complete");
		} else {
			playRandomSound("Problem writing ship settings", "Settings Write Error");
		}
	}


