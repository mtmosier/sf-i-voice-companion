	public void restoreSettings() {

		//*** Init
		bool? boolValueN;
		int? intValueN;
		string strValue, variable;
		string keybind;
		string settingName;
		string shipNameInput;
		string activeShipName = "";
		StringComparer comparer = StringComparer.CurrentCultureIgnoreCase;

