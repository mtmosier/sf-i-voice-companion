					for (short w = 0; w &lt; wgNameList.Length; w++) {
						for (short n = 1; n &lt;= maxWGNum; n++) {
							string fromVarName = "&gt;&gt;shipInfo[" + fromShip + "].weaponGroup[" + wgNameList[w] + "][" + n + "]";
